<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="389.0" prefWidth="808.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.willherofxfinal.controller">
   <children>
      <ImageView fitHeight="404.0" fitWidth="816.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@menu.jpg" />
         </image>
      </ImageView>
      <ButtonBar layoutX="-4.0" layoutY="345.0" prefHeight="40.0" prefWidth="816.0">
        <buttons>
            <Button mnemonicParsing="false" onAction="#switchtoLevel" prefHeight="28.0" prefWidth="340.0" text="NEW GAME" textFill="#1758e5">
               <font>
                  <Font name="Impact" size="15.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" text="LOAD" textFill="#1758e5">
               <font>
                  <Font name="Impact" size="15.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" text="INSTRUCTIONS" textFill="#1758e5">
               <font>
                  <Font name="Impact" size="15.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" text="LEADERBOARDS" textFill="#1758e5">
               <font>
                  <Font name="Impact" size="15.0" />
               </font>
            </Button>
          <Button mnemonicParsing="false" text="CREDITS" textFill="#1758e5">
               <font>
                  <Font name="Impact" size="15.0" />
               </font>
            </Button>
        </buttons>
      </ButtonBar>
      <Button layoutX="621.0" layoutY="14.0" mnemonicParsing="false" onAction="#exitfromGame" prefHeight="52.0" prefWidth="181.0" text="EXIT" textFill="#1758e5">
         <font>
            <Font name="Impact" size="27.0" />
         </font>
      </Button>
      <Button fx:id="highscore" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" prefHeight="52.0" prefWidth="181.0" text="HIGH SCORE" textFill="#1758e5">
         <font>
            <Font name="Impact" size="27.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
